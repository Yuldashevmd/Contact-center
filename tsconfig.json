{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/middleware.ts",
    "src/@core/service/context/main.jsx",
    "src/@core/service/hooks/useAuthContext.jsx",
    "src/@core/service/hooks/useAuth.jsx"
, "src/@core/utils/webVitals.js", "src/@core/apps/hooks/useDataContext.jsx", "src/@core/features/Chatbot/model/config/index.jsx", "src/@core/features/Chatbot/model/SaveMessages/index.js"  ],
  "exclude": ["node_modules"]
}
